------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Fantom Explorer" | get-tree | select fantest | get-menu Delete | click
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | click
    get-button OK | click
}
get-view "Fantom Explorer" | get-tree | select "TestProject/build.fan" | double-click
get-editor "build.fan" | get-editbox | get-property value 
    | equals "using buildclass Build : build::BuildPod{  new make()  {    podName = \"TestProject\"    summary = \"\"    srcDirs = [`fan/`]    depends = [\"sys 1.0\", \"fantest 1.0\"]  }}" 
    | verify-true
with [get-view "Fantom Explorer" | get-tree] {
    get-item TestProject | get-property caption | equals TestProject | verify-true
    get-item TestProject | get-property childCount | equals 5 | verify-true
    get-item "TestProject/Interpreter Libraries \\[fantom-1.0.68-embedded\\]" | get-property caption 
        | equals "Interpreter Libraries [fantom-1.0.68-embedded]" | verify-true
    get-item "TestProject/Interpreter Libraries \\[fantom-1.0.68-embedded\\]" | get-property childCount | equals 1 
        | verify-true
    get-item "TestProject/Interpreter Libraries \\[fantom-1.0.68-embedded\\]/sys.pod" | get-property caption 
        | equals "sys.pod" | verify-true
}
with [get-view Problems | get-tree] {
    get-property itemCount | equals 1 | verify-true
    get-item "Errors \\(1 item\\)" | get-property caption | equals "Errors (1 item)" | verify-true
    get-item "Errors \\(1 item\\)" | get-property childCount | equals 1 | verify-true
    get-item "Errors \\(1 item\\)/Cannot resolve depend: pod \'fantest\' not found" | get-property columns -index 0 
        | equals "Cannot resolve depend: pod \'fantest\' not found" | verify-true
    get-item "Errors \\(1 item\\)/Cannot resolve depend: pod \'fantest\' not found" | get-property columns -index 1 
        | equals TestProject | verify-true
    get-item "Errors \\(1 item\\)/Cannot resolve depend: pod \'fantest\' not found" | get-property columns -index 2 
        | equals "" | verify-true
    get-item "Errors \\(1 item\\)/Cannot resolve depend: pod \'fantest\' not found" | get-property columns -index 3 
        | equals "/TestProject" | verify-true
    get-item "Errors \\(1 item\\)/Cannot resolve depend: pod \'fantest\' not found" | get-property columns -index 4 
        | equals "Fantom Problem" | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
